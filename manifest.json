{
	"name": "Tabarmageddon",
	"version": "0.1",
	"description": "Lazy browsing people will get armageddon in tabs.",
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"browser_action": {
		"default_popup": "popup.html"
	},
	"options_ui": {
		"page": "options.html",
		"open_in_tab": false
	},
	"permissions": [
		"tabs",
		"storage"
	],
	"manifest_version": 2
}
